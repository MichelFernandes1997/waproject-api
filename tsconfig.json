{
   "compilerOptions": {
      "lib": [
         "ES2018"
      ],
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "moduleResolution": "node",
      "outDir": "./build",
      "sourceMap": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
   },
   "exclude": [
      "data"
   ]
}